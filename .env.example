# Copy to .env and fill values (do NOT commit your real .env)

# Local backup retention:
BACKUP_KEEP=7

# Optional AWS S3 upload (leave empty to disable)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1
S3_BUCKET=your-s3-bucket-name
S3_PREFIX=backups/

# (existing env vars)
ADMIN_PASS=changeme
JWT_SECRET=replace_with_strong_secret
# Note: For security, the real BOT_TOKEN should be stored in repository Secrets or server environment, not committed here.
BOT_TOKEN=
BOT_CHAT_ID=
PORT=3000
